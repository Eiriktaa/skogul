{
  "receivers": {
      "api": {
        "type": "http",
        "address": "[::1]:8080",
	"log204ok": true,
        "handlers": {
          "/": "jsontemplating"
        }
      }
  },
  "handlers": {
    "jsontemplating": {
      "parser": "json",
      "transformers": [  ],
      "sender": "count"
    },
    "debugger": {
      "parser": "json",
      "transformers": [],
      "sender": "print"
    }
  },
  "senders": {
    "batch": {
      "type": "batch",
      "threshold": 1000,
      "next": "count"
    },
    "count": {
      "type": "counter",
      "stats": "debugger",
      "next": "null"
    }
  }
}
