{
  "receivers": {
    "udp": {
      "type": "udp",
      "address": "[::1]:5015",
      "handler": "protobuf"
    }
  },
  "handlers": {
    "protobuf": {
      "parser": "protobuf",
      "transformers": ["metadata"],
      "sender": "print"
    }
  },
  "transformers": {
    "metadata": {
      "type": "metadata",
      "extractFromData": ["if_name"],
      "split": ["interfaceExp_stats"]
    }
  },
  "senders": {
    "print": {
      "type": "debug",
      "prefix": "DEBUG"
    }
  }
}